<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://wordcounter.com/" elementFormDefault="qualified" targetNamespace="http://wordcounter.com/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="CountWords">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="text" nillable="true" type="xs:string" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CountWordsResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="CountWordsResult" type="xs:int" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>